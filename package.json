{
    "author": "Simon Hampton",
    "name": "elm-webpack-starter",
    "version": "1.2.1",
    "description": "Elm starter with Webpack 4 hot-reloading",
    "main": "index.js",
    "scripts": {
        "test": "elm-verify-examples && elm-test",
        "start": "npm run dev",
        "dev": "webpack-dev-server --hot --port 3000 --colors",
        "build": "webpack",
        "prod": "webpack -p",
        "postinstall": "elm-package install -y && elm-test init"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/simonh1000/elm-webpack-starter.git"
    },
    "license": "MIT",
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-preset-env": "^1.7.0",
        "bootstrap-loader": "^2.2.0",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^0.28.11",
        "elm": "^0.18.0",
        "elm-hot-loader": "^0.5.4",
        "elm-verify-examples": "^2.3.1",
        "elm-webpack-loader": "^4.5.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.9.0",
        "resolve-url-loader": "^2.3.0",
        "sass-loader": "^7.0.1",
        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.8.3",
        "webpack-cli": "^2.1.3",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.2"
    },
    "dependencies": {
        "bootstrap": "4.1.1",
        "loglevel": "^1.6.1"
    }
}
